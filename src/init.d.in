#!/usr/bin/env /lib/init/init-d-script
### BEGIN INIT INFO
# Provides:          @PACKAGE@
# Required-Start:    $syslog $time $remote_fs
# Required-Stop:     $syslog $time $remote_fs
# Default-Start:     2 3 4 5
# Default-Stop:      0 1 6
# Short-Description: run at jobs
# Description:       Debian init script to start the daemon
#                    running at jobs.
### END INIT INFO
   
[ -f /etc/default/@PACKAGE@ ] && . /etc/default/@PACKAGE@
if [ -z "$exec_prefix" ] ;  then
	echo "exec_prefix is not set, please set it in /etc/default/@PACKAGE@ >&2
	exit 1
fi

DAEMON=@bindir@/@PACKAGE@
